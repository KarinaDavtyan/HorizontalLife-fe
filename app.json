{
  "expo": {
    "name": "HorizontalLife-fe",
    "description": "A very interesting project.",
    "slug": "HorizontalLife-fe",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  },
  "infoPlist": {
    "NSCameraUsageDescription": "This app uses the camera to take photos of the routes",
    "NSPhotoLibraryAddUsageDescription": "This app wants to have access to your library to choose existing route photo"
  }
}
